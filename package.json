{
  "name": "shared-operation-capital-big-screen",
  "private": true,
  "scripts": {
    "start": "cross-env umi dev",
    "start:test": "cross-env UMI_ENV=test umi dev",
    "start:prod": "cross-env UMI_ENV=prod umi dev",
    "build:dev": "cross-env  umi build",
    "build:test": "cross-env UMI_ENV=test umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "commit": "git-cz",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "changelog": "conventional-changelog -p angular -i .md -w -r 0",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "3d-earth": "^1.0.8",
    "@aec/big-screen": "0.0.71",
    "@aec/overview-card": "^0.0.46",
    "@aec/utils": "^0.0.10",
    "axios-mapper": "^0.5.6",
    "echarts-gl": "1.1.0",
    "events": "^3.3.0",
    "loadash": "^1.0.0",
    "loadjs": "^4.2.0",
    "numerable": "^0.3.15",
    "rc-queue-anim": "^2.0.0",
    "react": "17.x",
    "react-countup": "^6.1.1",
    "react-dom": "17.x",
    "react-fast-marquee": "^1.3.1",
    "react-spring": "^9.4.1",
    "react-use-websocket": "^3.0.0",
    "swiper": "^8.4.4",
    "umi": "^3.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/echarts": "^4.9.8",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/three": "^0.139.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.13",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "eslint": "^8.20.0",
    "eslint-config-alloy": "^4.6.2",
    "eslint-plugin-react": "^7.30.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.3",
    "typescript": "^4.7.4",
    "yorkie": "^2.0.0"
  }
}
